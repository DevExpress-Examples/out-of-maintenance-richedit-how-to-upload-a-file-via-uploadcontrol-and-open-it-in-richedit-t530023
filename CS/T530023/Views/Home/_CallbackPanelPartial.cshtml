@Html.DevExpress().CallbackPanel(
    settings => {
        settings.Name = "CallbackPanel";
        settings.CallbackRouteValues = new { Controller = "Home", Action = "CallbackPanelPartial" };
        settings.SetContent(() => 
        {
            Html.RenderAction("RichEditPartial", new { path = ViewData["path"] });
        });
    }).GetHtml()